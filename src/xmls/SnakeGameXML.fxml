<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="500.0" prefWidth="500.0" style="-fx-background-color: #563456;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <VBox alignment="CENTER" spacing="15.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="S N A K E" textFill="#fffefe">
               <font>
                  <Font name="Berlin Sans FB" size="25.0" />
               </font>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <graphic>
                  <ImageView fitHeight="150.0" fitWidth="70.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../images/Snek.png" />
                     </image>
                  </ImageView>
               </graphic>
               <effect>
                  <DropShadow height="25.0" radius="12.0" spread="0.15" width="25.0" />
               </effect>
            </Label>
         </children>
      </VBox>
   </top>
   <padding>
      <Insets bottom="65.0" top="20.0" />
   </padding>
</BorderPane>
